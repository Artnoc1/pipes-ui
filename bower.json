{
  "name": "toggl-pipes-ui",
  "version": "0.3.0",
  "main": "public/app.js",
  "dependencies": {
    "jquery": ">=2.0.3",
    "lodash": "~2.4.1",
    "backbone": "~1.1.2",
    "font-awesome": "~4.0.3",
    "momentjs": "~2.5.1",
    "foundation": "foundation#master",
    "iframe-resizer": "~2.4.6"
  },
  "devDependencies": {},
  "overrides": {
    "modernizr": {
      "main": "modernizr.js"
    },
    "jquery-placeholder": {
      "main": "jquery-placeholder.js"
    },
    "iframe-resizer": {
      "main": "js/iframeResizer.contentWindow.min.js"
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/FontAwesome.otf",
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff"
      ]
    },
    "foundation": {
      "main": [
        "js/foundation.js",
        "scss/foundation.scss"
      ],
      "_comment": "We want to configure foundation and compile manually: use scss instead of css"
    }
  }
}
